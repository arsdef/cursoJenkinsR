\\pipeline que sume, reste, multiplique y divida
pipeline
{
    
    agent any
    stages
    {
        stage("suma")
        {
            
            steps
            {
                script
                {
                    def numero = 1
                    def numero_2 = 2
                    def suma = numero + numero_2
                    println   suma
                }
            }
        }
        stage("resta")
        {
            
            steps
            {
                script
                {
                    def numero = 1
                    def numero_2 = 2
                    def resta = numero - numero_2
                    println   resta
                }
            }
        } 
        stage("multiplicacion")
        {
            
            steps
            {
                script
                {
                    def numero = 1
                    def numero_2 = 2
                    def multiplicacion= numero * numero_2
                    println   multiplicacion
                }
            }
        }
         stage("division")
        {
            
            steps
            {
                script
                {
                    def numero = 1
                    def numero_2 = 2
                    def division = numero \ numero_2
                    print   division
                }
            }
        }
    }
}